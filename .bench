set_env AVM_HOME "$HOME/.avm"
set_env CARGO_HOME "$HOME/.cargo"
set_env NODE_OPTIONS --no-deprecation
set_env SOLANA_HOME "$HOME/.local/share/solana/install/active_release"

append_env PATH : "$AVM_HOME/bin"
append_env PATH : "$CARGO_HOME/bin"
append_env PATH : "$SOLANA_HOME/bin"

check_dir "$AVM_HOME"
check_dir "$CARGO_HOME"
check_dir "$SOLANA_HOME"
